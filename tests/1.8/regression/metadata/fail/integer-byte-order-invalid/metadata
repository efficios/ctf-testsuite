/* CTF 1.8 */

typealias integer { size = 32; align = 8; signed = false; } := uint32_t;

/* FAIL: invalid byte ordering. */
typealias integer { size = 8; align = 8; signed = false; byte_order = iroquois; } := uint8_t;

trace {
	major = 1;
	minor = 8;
	byte_order = le;
	packet.header := struct {
		uint32_t magic;
	};
};
