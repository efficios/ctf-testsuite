/* CTF 1.8 */

typealias integer { size = 32; align = 8; signed = false; } := uint32_t;

/* PASS: struct should be aligned to 16 bits (follows the specified value). */
struct dummy {
  uint32_t field1;
  uint32_t field2;
  uint32_t field3;
} align(16);


trace {
	major = 1;
	minor = 8;
	byte_order = le;
	packet.header := struct {
		uint32_t magic;
	};
};
