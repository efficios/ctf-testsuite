/* CTF 1.8 */

typealias integer { size = 32; align = 8; signed = false; } := uint32_t;

/* PASS: struct typedef */
typedef struct {
	uint32_t data;
} my_struct;

trace {
	major = 2;
	minor = 1;
	byte_order = le;
	packet.header := struct {
		uint32_t magic;
	};
};

event {
	name = "event";
	fields := my_struct;
};
