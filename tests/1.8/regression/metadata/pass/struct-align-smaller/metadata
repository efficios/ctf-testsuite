/* CTF 1.8 */

typealias integer { size = 32; align = 8; signed = false; } := uint32_t;

/*
 * PASS: struct should be aligned to 8 bits (fields' value is larger than the
 * one specified by the struct).
 */
struct dummy {
  uint32_t field1;
  uint32_t field2;
  uint32_t field3;
} align(4);


trace {
	major = 1;
	minor = 8;
	byte_order = le;
	packet.header := struct {
		uint32_t magic;
	};
};
