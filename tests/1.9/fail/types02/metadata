/* CTF 1.8 */

typealias integer { size = 32; align = 8; signed = false; } := uint32_t;

/* FAIL: Alignment values must be power of two. */
typealias integer { size = 8; align = 17; signed = false; } := uint0_t;

trace {
	major = 2;
	minor = 1;
	byte_order = le;
	packet.header := struct {
		uint32_t magic;
	};
};
